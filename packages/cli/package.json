{
  "name": "cforty-cli",
  "version": "1.0.8",
  "description": "A CLI for Cforty.",
  "type": "module",
  "exports": "./dist/index.js",
  "bin": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "start": "node dist/index.js",
    "format": "pnpm biome format . --write",
    "typecheck": "tsc --noEmit",
    "pub:beta": "pnpm build && pnpm publish --no-git-checks --access public --tag beta",
    "pub:release": "pnpm build && pnpm publish --access public"
  },
  "author": "Lukas Jaronis",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^1.5.2",
    "@clack/core": "^0.3.3",
    "@clack/prompts": "^0.7.0",
    "@types/prompts": "^2.4.9",
    "chalk": "^5.3.0",
    "pnpm": "^8.14.1",
    "tsup": "^8.0.1",
    "type-fest": "^4.9.0"
  },
  "dependencies": {
    "@types/fs-extra": "^11.0.4",
    "cli-table3": "^0.6.3",
    "clipboardy": "^4.0.0",
    "commander": "^11.1.0",
    "execa": "^8.0.1",
    "fs-extra": "^11.2.0",
    "ora": "^8.0.1",
    "prompts": "^2.4.2",
    "zod": "^3.22.4"
  }
}
